{
  "name": "miaoxing/queue",
  "description": "Miaoxing Queue Plugin",
  "keywords": [
    "miaoxing",
    "queue"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.7.7"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Queue\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Queue\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "miaoxing/dev": "^8.2.3"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev"
    }
  }
}
